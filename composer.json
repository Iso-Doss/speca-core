{
    "name": "speca/speca-core",
    "description": ":package_description",
    "keywords": [
        "speca",
        "laravel",
        "speca-code"
    ],
    "homepage": "https://github.com/Iso-Doss/speca-core",
    "license": "MIT",
    "authors": [
        {
            "name": "IsraÃ«l DOSSOU",
            "email": "israel.dossou48@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Iso-Doss/speca-core/issues",
        "source": "https://github.com/Iso-Doss/speca-core"
    },
    "require": {
        "php": "^8.4",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "barryvdh/laravel-dompdf": "^3.1",
        "bugsnag/bugsnag-laravel": "^2.29",
        "dedoc/scramble": "^0.13.14",
        "doctrine/dbal": "^3.6",
        "giggsey/libphonenumber-for-php": "^8.13",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/contracts": "^11.0||^12.0",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel-lang/common": "^6.4",
        "laravel/framework": "^12.0",
        "laravel/passport": "^13.4.3",
        "laravel/pulse": "^1.4",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.24",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.10.1",
        "laravel/wayfinder": "^0.1.9",
        "league/flysystem-aws-s3-v3": "^3.28",
        "maatwebsite/excel": "^3.1",
        "nuzkito/chrome-html-to-pdf": "^2.0",
        "opcodesio/log-viewer": "^3.11",
        "predis/predis": "^3.4",
        "pusher/pusher-php-server": "^7.2",
        "spatie/image": "^3.7",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-package-tools": "^1.16",
        "spatie/laravel-pdf": "^1.5",
        "spatie/laravel-permission": "^6.3",
        "spatie/pdf-to-image": "^1.2",
        "tightenco/ziggy": "^2.4",
        "laravel/fortify": "^1.30"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "nunomaduro/larastan": "^3.9",
        "orchestra/testbench": "^10.9",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^11.5",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-ray": "^1.43"
    },
    "autoload": {
        "psr-4": {
            "Speca\\SpecaCore\\": "src/",
            "Speca\\SpecaCore\\Database\\Factories\\": "database/factories/",
            "Speca\\SpecaCore\\Database\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Speca\\SpecaCore\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Speca\\SpecaCore\\SpecaCoreServiceProvider"
            ],
            "aliases": {
                "SpecaCore": "Speca\\SpecaCore\\Facades\\SpecaCore"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
